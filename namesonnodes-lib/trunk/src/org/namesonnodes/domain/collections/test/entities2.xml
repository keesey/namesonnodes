	<TaxonIdentifier localName="Metazoa">
		<authority>
			<refAuthority>org.namesonnodes.domain.collections.test.DatasetCollectionTest</refAuthority>
		</authority>
		<entity>
			<Taxon>
				<definition>
					<PhyloDefinition>
						<formula>
							<math:math>
								<math:apply>
									<math:csymbol definitionURL="http://namesonnodes.org/ns/math/2009#def-NodeBasedClade"/>
									<math:apply>
										<math:union/>
										<math:csymbol definitionURL="urn:isbn:0853010064::Spongia+botryoides"/>
										<math:csymbol definitionURL="urn:isbn:0853010064::Oopsacas+minuta"/>
										<math:csymbol definitionURL="urn:isbn:0853010064::Spongia+officinalis"/>
										<math:csymbol definitionURL="urn:isbn:0853010064::Homo+sapiens"/>
									</math:apply>
								</math:apply>
							</math:math>
						</formula>
						<prose>
							<html:div>All successors of the maximal common predecessors of <html:i>Oppsacas minuta</html:i> Topsent 1927, <html:i>Spongia botryoides</html:i> Ellis &amp; Solander 1786 (=<html:i>Leucosolenia botryoides</html:i>), <html:i>Spongia officinalis</html:i> Linnaeus 1758, and <html:i>Homo sapiens</html:i> Linnaeus 1758.</html:div>
						</prose>
						<specifiers>
							<refTaxon>urn:isbn:0853010064::Homo+sapiens</refTaxon>
							<refTaxon>urn:isbn:0853010064::Oopsacas+minuta</refTaxon>
							<refTaxon>urn:isbn:0853010064::Spongia+botryoides</refTaxon>
							<refTaxon>urn:isbn:0853010064::Spongia+officinalis</refTaxon>
						</specifiers>
					</PhyloDefinition>
				</definition>
			</Taxon>
		</entity>
		<label>
			<Label>
				<name>Metazoa</name>
				<italics>true</italics>
			</Label>
		</label>
	</TaxonIdentifier>
	<TaxonIdentifier localName="Porifera">
		<authority>
			<refAuthority>org.namesonnodes.domain.collections.test.DatasetCollectionTest</refAuthority>
		</authority>
		<entity>
			<Taxon>
				<definition>
					<PhyloDefinition>
						<formula>
							<math:math>
								<math:apply>
									<math:csymbol definitionURL="http://namesonnodes.org/ns/math/2009#def-CrownGroup"/>
									<math:apply>
										<math:csymbol definitionURL="http://namesonnodes.org/ns/math/2009#def-BranchBasedClade"/>
										<math:apply>
											<math:union/>
											<math:csymbol definitionURL="urn:isbn:0853010064::Spongia+botryoides"/>
											<math:csymbol definitionURL="urn:isbn:0853010064::Oopsacas+minuta"/>
											<math:csymbol definitionURL="urn:isbn:0853010064::Spongia+officinalis"/>
										</math:apply>
										<math:csymbol definitionURL="urn:isbn:0853010064::Homo+sapiens"/>
									</math:apply>
								</math:apply>
							</math:math>
						</formula>
						<prose>
							<html:div>All successors of the maximal common predecessors of all extant organisms sharing closer ancestry with <html:i>Oppsacas minuta</html:i> Topsent 1927, <html:i>Spongia botryoides</html:i> Ellis &amp; Solander 1786 (=<html:i>Leucosolenia botryoides</html:i>), and <html:i>Spongia officinalis</html:i> Linnaeus 1758 than with <html:i>Homo sapiens</html:i> Linnaeus 1758.</html:div>
						</prose>
						<specifiers>
							<refTaxon>urn:isbn:0853010064::Homo+sapiens</refTaxon>
							<refTaxon>urn:isbn:0853010064::Oopsacas+minuta</refTaxon>
							<refTaxon>urn:isbn:0853010064::Spongia+botryoides</refTaxon>
							<refTaxon>urn:isbn:0853010064::Spongia+officinalis</refTaxon>
						</specifiers>
					</PhyloDefinition>
				</definition>
			</Taxon>
		</entity>
		<label>
			<Label>
				<name>Metazoa</name>
				<italics>true</italics>
			</Label>
		</label>
	</TaxonIdentifier>
