		<entity>
			<Taxon>
				<definition>
					<PhyloDefinition>
						<formula>
							<math xmlns="http://www.w3.org/1998/Math/MathML">
								<apply>
									<csymbol definitionURL="http://namesonnodes.org/ns/math/2009#def-Clade"/>
									<apply>
										<csymbol definitionURL="http://namesonnodes.org/ns/math/2009#def-Minimal"/>
										<apply>
											<intersect/>
											<apply>
												<csymbol definitionURL="http://namesonnodes.org/ns/math/2009#def-SynapomorphicPredecessors"/>
												<apply>
													<intersect/>
													<csymbol definitionURL="urn:doi:10.1206/0003-0090(2004)286&lt;0001:MPTASO&gt;2.0.CO;2::Diagnosis:Apomorphy:2"/>
													<csymbol definitionURL="urn:doi:10.1206/0003-0090(2004)286&lt;0001:MPTASO&gt;2.0.CO;2::Diagnosis:Apomorphy:5"/>
